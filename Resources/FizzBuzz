
// FizzBuzz
// Write a function that goes through the numbers 1 - 100 and:
// each number that is a multiple of 3, we print 'Fizz'
// each number that is a multiple of 5, we print 'Buzz'
// each number that is a multiple of 3 and 5, we print 'FizzBuzz'
// all other numbers we print just the number

function fizzBuzz() {
  // go from 1 to 100 and repeatedly check the numbers if they are a multiple
  // loop through the numbers 1 - 100
  for (var i = 1; i <= 100; i++) {
    // check if divisible by 3 and 5 (where should this go?)
    if (i % 3 == 0 && i % 5 == 0) {
      console.log('FizzBuzz')
    }
    // if not divisble by 3, check if divisible by 5
    else if (i % 5 == 0) {
      console.log('Buzz Lightyear')
    }
    // check if the number is divisble by 3
    else if (i % 3 == 0) {
      console.log('Fizz')
    }
    // else print the number
    else {
      console.log(i)
    }
  }
}

// fizzBuzz()
